<items 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
			xsi:noNamespaceSchemaLocation="items.xsd">

    <itemtypes>
            
        <typegroup name="CustomPaymentInfo">
            <itemtype code="CreditCardPaymentInfo" autocreate="false" generate="false">
                <description>Extending CreditCardPaymentInfo type with additional attributes to Mundipagg adding key "MundipaggStantBuyKey" and "Document" for subscription.</description>
                <attributes>
                    <attribute qualifier="mundipaggStantBuyKey" type="java.lang.String">
                        <description> Mundipagg SatantBuyKey. </description>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="cpfForMundipagg" type="java.lang.String">
                        <description> Document (CPF) for mundipagg. </description>
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>
            
    <itemtype code="MundipaggBoletoPaymentInfo" extends="PaymentInfo" jaloclass="com.flieger.payment.jalo.MundipaggBoletoPaymentInfo">
        <attributes>
            <attribute type="java.lang.String" qualifier="transactionReference">
                <description>Identifier of the transaction by store </description>
                <modifiers optional="false" />
                <persistence type="property" />
            </attribute>
            <attribute type="java.lang.String" qualifier="nossoNumero">
                <description>Número de identificação do boleto</description>
                <modifiers optional="false" />
                <persistence type="property" />
            </attribute>
            <attribute type="java.lang.String" qualifier="daysToAddInBoletoExpirationDate">
                <description>Days to add in boleto expiration date</description>
                <modifiers optional="false" />
                <persistence type="property" />
            </attribute>
             <attribute qualifier="cpfForMundipagg" type="java.lang.String">
                        <description> Document (CPF) for mundipagg. </description>
                        <persistence type="property"/>
             </attribute>
       </attributes>		
    </itemtype>
        </typegroup>
		
        <typegroup name="CustomPaymentTransactionEntry">
        <itemtype code="PaymentTransactionEntry" autocreate="false" generate="false">
			<attributes>
				<attribute qualifier="mundipaggRequestKey" type="java.lang.String">
					<persistence type="property"></persistence>
				</attribute>
				<attribute qualifier="mundipaggOrderKey" type="java.lang.String">
					<persistence type="property"></persistence>
				</attribute>
				<attribute qualifier="mundipaggTransactionKey" type="java.lang.String">
					<persistence type="property"/>
				</attribute>
                                <attribute qualifier="mundipaggOrderReference" type="java.lang.String">
					<persistence type="property"/>
				</attribute>
                                <attribute qualifier="mundipaggBoletoBarCode" type="java.lang.String">
					<persistence type="property"/>
				</attribute>
                                <attribute qualifier="mundipaggBoletoStatus" type="java.lang.String">
					<persistence type="property"/>
				</attribute>
                                <attribute qualifier="mundipaggBoletoUrl" type="java.lang.String">
					<persistence type="property"/>
				</attribute>
                                <attribute qualifier="mundipaggBoletoNossoNumero" type="java.lang.String">
					<persistence type="property"/>
				</attribute>
                                <attribute qualifier="mundipaggTransactionReference" type="java.lang.String">
					<persistence type="property"/>
				</attribute>
                                
			</attributes>
                </itemtype>
        </typegroup>
