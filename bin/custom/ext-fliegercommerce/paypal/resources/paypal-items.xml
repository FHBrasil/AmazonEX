<items xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="items.xsd">
    <enumtypes>
        <enumtype code="PaymentTransactionType" autocreate="false"
            generate="false">
            <description>PayPal specific payment transaction type</description>
            <value code="ORDER" />
            <value code="REAUTHORIZATION"></value>
            <value code="SALE" />
        </enumtype>
        <enumtype code="PaymentActionType" autocreate="true" generate="true">
            <description>Paypal available values for payment action</description>
            <value code="ORDER" />
            <value code="AUTHORIZATION" />
            <value code="SALE" />
        </enumtype>
    </enumtypes>

    <itemtypes>
        <itemtype code="PaypalPaymentInfo" extends="PaymentInfo"
            jaloclass="br.hering.core.jalo.order.payment.PayPalPaymentInfo"
            autocreate="true" generate="true">
            <attributes>
                <attribute qualifier="token" type="java.lang.String">
                    <description>Token for PayPal operations chain</description>
                    <persistence type="property"></persistence>
                    <modifiers read="true" write="true" optional="true" />
                </attribute>
                <attribute qualifier="paymentAction" type="PaymentActionType">
                    <description>Payment action for PayPal operations chain</description>
                    <persistence type="property"></persistence>
                    <modifiers read="true" write="true" optional="false" />
                </attribute>
                <attribute qualifier="payerid" type="java.lang.String">
                    <description>PayerId for DoExpressCheckoutPayment PayPal operation</description>
                    <persistence type="property"></persistence>
                    <modifiers read="true" write="true" optional="true" />
                </attribute>
                <!-- <attribute type="java.lang.String" qualifier="payeremail"> -->
                <!-- <description>Email of user involved in transaction</description> -->
                <!-- <modifiers optional="true" /> -->
                <!-- <persistence type="property" /> -->
                <!-- </attribute> -->
                <attribute qualifier="payer" type="java.lang.String">
                    <description>Payer for payment information display</description>
                    <persistence type="property"></persistence>
                    <modifiers read="true" write="true" optional="true" />
                </attribute>
                <attribute qualifier="billingAgreementID" type="java.lang.String">
                    <persistence type="property"></persistence>
                    <modifiers read="true" write="true" optional="true" />
                </attribute>
                <attribute qualifier="useReferenceTransaction" type="java.lang.Boolean">
                    <description>Defines if the reference transaction(billing agreement) is used for order</description>
                    <persistence type="property" />
                    <modifiers read="true" write="true" optional="true" />
                </attribute>
                <attribute type="java.lang.String" qualifier="transactionID">
                    <description>Returned transactionID from PayPal</description>
                    <modifiers optional="true" />
                    <persistence type="property" />
                </attribute>
                <attribute type="java.lang.String" qualifier="orderTime">
                    <description>Returned Order Time from PayPal</description>
                    <modifiers optional="true" />
                    <persistence type="property" />
                </attribute>
                <attribute type="java.lang.String" qualifier="paymentStatus">
                    <description>Returned payment status from PayPal</description>
                    <modifiers optional="true" />
                    <persistence type="property" />
                </attribute>
                <attribute type="java.lang.String" qualifier="pendingReason">
                    <description>Returned pending reason from PayPal</description>
                    <modifiers optional="true" />
                    <persistence type="property" />
                </attribute>
                <attribute type="java.lang.String" qualifier="reasonCode">
                    <description>Returned reason code from PayPal</description>
                    <modifiers optional="true" />
                    <persistence type="property" />
                </attribute>
                <attribute type="java.lang.String" qualifier="approvalResult">
                    <description>Returned approval result from PayPal</description>
                    <modifiers optional="true" />
                    <persistence type="property" />
                </attribute>
                <attribute type="java.lang.String" qualifier="error">
                    <description>Returned error from PayPal</description>
                    <modifiers optional="true" />
                    <persistence type="property" />
                </attribute>
                <attribute type="java.lang.String" qualifier="errorDetails">
                    <description>Returned error details from PayPal</description>
                    <modifiers optional="true" />
                    <persistence type="property" />
                </attribute>
            </attributes>
        </itemtype>
    </itemtypes>
</items>
