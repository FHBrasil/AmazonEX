<?com.pixi.webservices.jaxb version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="BMECAT">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="HEADER"/>
        <xs:element ref="T_NEW_CATALOG"/>
      </xs:sequence>
      <xs:attribute name="version" use="required" type="xs:decimal"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="HEADER">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="GENERATOR_INFO"/>
        <xs:element ref="CATALOG"/>
        <xs:element ref="SUPPLIER"/>
        <xs:element ref="BUYER"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="GENERATOR_INFO" type="xs:string"/>
  <xs:element name="CATALOG">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="LANGUAGE"/>
        <xs:element ref="CATALOG_ID"/>
        <xs:element ref="CATALOG_VERSION"/>
        <xs:element ref="CATALOG_NAME"/>
        <xs:element ref="CURRENCY"/>
        <xs:element ref="DATABASE"/>
        <xs:element ref="SHOPID"/>
        <xs:element ref="DATEEXPORT"/>
        <xs:element ref="EXPORT_DATE"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="LANGUAGE" type="xs:string"/>
  <xs:element name="CATALOG_ID" type="xs:string"/>
  <xs:element name="CATALOG_VERSION" type="xs:string"/>
  <xs:element name="CATALOG_NAME" type="xs:string"/>
  <xs:element name="CURRENCY" type="xs:string"/>
  <xs:element name="DATABASE" type="xs:string"/>
  <xs:element name="SHOPID" type="xs:string"/>
  <xs:element name="DATEEXPORT" type="xs:date"/>
  <xs:element name="EXPORT_DATE" type="xs:date"/>
  <xs:element name="SUPPLIER">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="SUPPLIER_NAME" type="xs:string"/>
        <xs:element ref="ADDRESS"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="BUYER">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="BUYER_ID"/>
        <xs:element ref="BUYER_NAME"/>
        <xs:element ref="ADDRESS"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="BUYER_ID" type="xs:string"/>
  <xs:element name="BUYER_NAME" type="xs:string"/>
  <xs:element name="T_NEW_CATALOG">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="ARTICLE"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ARTICLE">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="SUPPLIER_AID"/>
        <xs:element ref="ARTICLE_DETAILS"/>
        <xs:element ref="ARTICLE_FEATURES"/>
        <xs:element ref="ARTICLE_PRICE_DETAILS"/>
        <xs:element ref="ITEMTAGS"/>
        <xs:element ref="MIME_INFO"/>
        <xs:element ref="ARTICLE_ORDER_DETAILS"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="SUPPLIER_AID" type="xs:string"/>
  <xs:element name="ARTICLE_DETAILS">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="DESCRIPTION_SHORT"/>
        <xs:element ref="EAN"/>
        <xs:element ref="MANUFACTURER_NAME"/>
        <xs:element ref="SEGMENT"/>
        <xs:element ref="WEIGHT"/>
        <xs:element ref="INTERNAL_ITEM_NUMBER"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="DESCRIPTION_SHORT" type="xs:string"/>
  <xs:element name="EAN" type="xs:string"/>
  <xs:element name="MANUFACTURER_NAME" type="xs:string"/>
  <xs:element name="SEGMENT" type="xs:string"/>
  <xs:element name="WEIGHT" type="xs:decimal"/>
  <xs:element name="INTERNAL_ITEM_NUMBER" type="xs:string"/>
  <xs:element name="ARTICLE_FEATURES">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="FEATURE"/>
        <xs:element ref="SerialNumberRequired"/>
        <xs:element ref="CustomsTariffNumber"/>
        <xs:element ref="CustomsCountryOfOrigin"/>
        <xs:element ref="CustomsTariffText"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="FEATURE">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="FNAME"/>
        <xs:element ref="FVALUE"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="FNAME" type="xs:string"/>
  <xs:element name="FVALUE" type="xs:string"/>
  <xs:element name="SerialNumberRequired" type="xs:string"/>
  <xs:element name="CustomsTariffNumber" type="xs:string"/>
  <xs:element name="CustomsCountryOfOrigin" type="xs:string"/>
  <xs:element name="CustomsTariffText" type="xs:string"/>
  <xs:element name="ARTICLE_PRICE_DETAILS">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="ARTICLE_PRICE"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ARTICLE_PRICE">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="PRICE_AMOUNT"/>
        <xs:element ref="PRICE_CURRENCY"/>
        <xs:element ref="SUPPLPRICE_AMOUNT"/>
      </xs:sequence>
      <xs:attribute name="price_type" use="required" type="xs:string"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="PRICE_AMOUNT" type="xs:decimal"/>
  <xs:element name="PRICE_CURRENCY" type="xs:string"/>
  <xs:element name="SUPPLPRICE_AMOUNT" type="xs:decimal"/>
  <xs:element name="ITEMTAGS">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="ITEMTAG"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ITEMTAG" type="xs:string"/>
  <xs:element name="MIME_INFO">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="MIME"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="MIME">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="MIME_TYPE"/>
        <xs:element ref="MIME_SOURCE"/>
        <xs:element ref="MIME_PURPOSE"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="MIME_TYPE" type="xs:string"/>
  <xs:element name="MIME_SOURCE" type="xs:anyURI"/>
  <xs:element name="MIME_PURPOSE" type="xs:string"/>
  <xs:element name="ARTICLE_ORDER_DETAILS">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ORDER_UNIT"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ORDER_UNIT" type="xs:string"/>
  <xs:element name="ADDRESS">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="NAME"/>
        <xs:element ref="NAME2"/>
        <xs:element ref="STREET"/>
        <xs:element ref="ZIP"/>
        <xs:element ref="CITY"/>
        <xs:element ref="COUNTRY"/>
      </xs:sequence>
      <xs:attribute name="type" use="required" type="xs:string"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="NAME" type="xs:string"/>
  <xs:element name="NAME2" type="xs:string"/>
  <xs:element name="STREET" type="xs:string"/>
  <xs:element name="ZIP" type="xs:string"/>
  <xs:element name="CITY" type="xs:string"/>
  <xs:element name="COUNTRY" type="xs:string"/>
</xs:schema>
