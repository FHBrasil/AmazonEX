				</entry>
				<entry>
					<key><value>md5</value></key>
					<ref bean="core.saltedMD5PasswordEncoder"/>
				</entry>
	  			<entry>
					<key><value>sha256</value></key>
					<ref bean="core.saltedSHA256PasswordEncoder"/>	
				</entry>	
			</map>
		</property>
	</bean>
	
