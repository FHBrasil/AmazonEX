	<itemtypes>
		<typegroup name="Hering">
			<itemtype code="BaseStore" autocreate="false" generate="false">
				<description>Extending BaseStore type with additional attributes.</description>
				<attributes>
					<attribute type="java.lang.Double" qualifier="freeDeliveryThreshold">
						<persistence type="property"/>
					</attribute>
					<attribute type="java.lang.String" qualifier="linxSiteCode">
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>
			
			<itemtype code="HeringWishlist" extends="Wishlist2" autocreate="true" generate="true" jaloclass="br.hering.core.jalo.HeringWishlist">
				<attributes>
					<attribute qualifier="publicName" type="java.lang.String">
						<modifiers read="true" write="true" optional="true" />
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>
			
			<itemtype code="HeringProduct" extends="Product"
			          autocreate="true" generate="true"
			          jaloclass="br.hering.core.jalo.HeringProduct">
				<description>Base hering product extension that contains additional attributes.</description>
				<attributes>
					<attribute qualifier="genders" type="GenderList">
						<description>List of genders that the HeringProduct is designed for</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="measurementList" type="MeasurementsList">
						<description>List of measurements of the HeringProduct</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>
			<itemtype code="Product"
			          autocreate="false" generate="false">
				<attributes>
					<attribute qualifier="oldPrice" type="java.math.BigDecimal">
						<description>Original price of the product</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="sale30" type="int">
						<description>Quantity of sales in 30 days</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="sale30date" type="java.util.Date">
						<description>Date verification of sales' quantity in 30 days</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>
			<itemtype code="Address"
			          autocreate="false" generate="false">
				<description>Base hering product extension that contains additional attributes.</description>
				<attributes>
					<attribute qualifier="dddPhone" type="localized:java.lang.String">
						<description>DDD of the telephone. </description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="dddCellPhone" type="localized:java.lang.String">
						<description>DDD of the cellphone. </description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="complemento" type="localized:java.lang.String">
						<description>Observation of the address. </description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="tipoDeEndereco" type="TipoDeEndereco">
						<description>Address type (comercial/residencial). </description>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="pontoDeReferencia" type="localized:java.lang.String">
						<description>Reference of the Address. </description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="receiver" type="localized:java.lang.String">
						<description>Receiver of the Address. </description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>
			<itemtype code="HeringStyleVariantProduct" extends="VariantProduct"
			          autocreate="true" generate="true"
			          jaloclass="br.hering.core.jalo.HeringStyleVariantProduct">
				<description>Hering style variant type that contains additional attribute describing variant style.
				</description>
				<attributes>
					<attribute qualifier="style" type="localized:java.lang.String"
					           metatype="VariantAttributeDescriptor">
						<description>Color/Pattern of the product.</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="swatchColors" type="SwatchColorSet">
						<description>A normalized color mapping to a standardized front-end navigable name.
						</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>
			<itemtype code="HeringSizeVariantProduct" extends="HeringStyleVariantProduct"
			          autocreate="true" generate="true"
			          jaloclass="br.hering.core.jalo.HeringSizeVariantProduct">
				<description>Hering size variant type that contains additional attribute describing variant size.</description>
				<attributes>
					<attribute qualifier="size" type="localized:java.lang.String" metatype="VariantAttributeDescriptor">
						<description>Size of the product.</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute type="int" qualifier="position">
						<description>Defines the ordering of the skus</description>
						<persistence type="property"/>
					</attribute>
					<attribute type="double" qualifier="weight">
				    	<persistence type="dynamic"/>
				    	<modifiers write="false"/>
				  	</attribute>
					<attribute type="java.lang.String" qualifier="colorRGB">
				    	<persistence type="dynamic"/>
				    	<modifiers write="false"/>
				  	</attribute>
				  	<attribute type="java.lang.String" qualifier="colorBasicDescription">
				    	<persistence type="dynamic"/>
				    	<modifiers write="false"/>
				  	</attribute>
				  	<attribute type="java.lang.String" qualifier="colorFullDescription">
				    	<persistence type="dynamic"/>
				    	<modifiers write="false"/>
				  	</attribute>
				</attributes>
			</itemtype>
			
			<itemtype code="ProductMeasurements">
				<deployment table="ProductMeasurements" typecode="10004"/>
				<attributes>
					<attribute type="localized:java.lang.String" qualifier="size">
						<persistence type="property"/>
					</attribute>
					<attribute type="localized:java.lang.String" qualifier="type">
						<persistence type="property"/>
					</attribute>
					<attribute type="localized:java.lang.Double" qualifier="measurement">
						<persistence type="property"/>
					</attribute>
					<attribute type="localized:Media" qualifier="picture">
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>
		</typegroup>
		
		<typegroup name="AcceleratorBR">
			<itemtype code="Customer" autocreate="false" generate="false">
				<description>Base hering product extension that contains additional attributes.</description>
				<attributes>
					<attribute qualifier="birthday" type="java.util.Date">
						<modifiers read="true" write="true" optional="true" />
						<persistence type="property"/>
					</attribute>
					
					<attribute qualifier="cpfcnpj" type="java.lang.String">
						<description>CPF or CNPF of the customer.</description>
						<modifiers read="true" write="true" optional="true" />
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="rgIe" type="localized:java.lang.String">
						<description>Number of the document. </description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="ufIe" type="java.lang.String">
						<description>Uf of the Ie. </description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="registrationDate" type="localized:java.util.Date">
						<description>Date of registration. </description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="gender" type="Gender">
						<description>Gender of the customer </description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="facebookId" type="java.lang.Long">
						<description>Facebook ID. </description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>
			<itemtype code="AbstractOrder" autocreate="false" generate="false">
				<attributes>
					<attribute qualifier="estimatedDeliveryDays" type="int">
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>
		</typegroup>
	
		<typegroup name="CustomPaymentModes">
			<itemtype code="PayPalPaymentInfo" extends="PaymentInfo" jaloclass="br.hering.core.jalo.order.payment.PayPalPaymentInfo">
			<attributes>
				<attribute type="java.lang.String" qualifier="token">
					<description>Token ID of the PayPal session</description>
					<modifiers optional="false" />
					<persistence type="property" />
				</attribute>
				<attribute type="java.lang.String" qualifier="payerid">
					<description>PayPal ID of user involved in transaction</description>
					<modifiers optional="false" />
					<persistence type="property" />
				</attribute>
				<attribute type="java.lang.String" qualifier="payeremail">
					<description>Email of user involved in transaction</description>
					<modifiers optional="false" />
					<persistence type="property" />
				</attribute>
				<attribute type="java.lang.String" qualifier="transactionID">
					<description>Returned transactionID from PayPal</description>
					<modifiers optional="true" />
					<persistence type="property" />
				</attribute>
				<attribute type="java.lang.String" qualifier="orderTime">
					<description>Returned Order Time from PayPal</description>
					<modifiers optional="true" />
					<persistence type="property" />
				</attribute>
				<attribute type="java.lang.String" qualifier="paymentStatus">
					<description>Returned payment status from PayPal</description>
					<modifiers optional="true" />
					<persistence type="property" />
				</attribute>
				<attribute type="java.lang.String" qualifier="pendingReason">
					<description>Returned pending reason from PayPal</description>
					<modifiers optional="true" />
					<persistence type="property" />
				</attribute>
				<attribute type="java.lang.String" qualifier="reasonCode">
					<description>Returned reason code from PayPal</description>
					<modifiers optional="true" />
					<persistence type="property" />
				</attribute>
				<attribute type="java.lang.String" qualifier="approvalResult">
					<description>Returned approval result from PayPal</description>
					<modifiers optional="true" />
					<persistence type="property" />
				</attribute>
				<attribute type="java.lang.String" qualifier="error">
					<description>Returned error from PayPal</description>
					<modifiers optional="true" />
					<persistence type="property" />
				</attribute>
				<attribute type="java.lang.String" qualifier="errorDetails">
					<description>Returned error details from PayPal</description>
					<modifiers optional="true" />
					<persistence type="property" />
				</attribute>
			</attributes>
			</itemtype>			
			<itemtype code="VoucherPaymentInfo" extends="PaymentInfo" jaloclass="br.hering.core.jalo.order.payment.VoucherPaymentInfo">
				<attributes>
					<attribute type="java.lang.String" qualifier="voucher">
						<description>Vocher Code</description>
						<modifiers optional="true" />
						<persistence type="property" />
					</attribute>
					<attribute type="java.lang.Double" qualifier="voucherValue">
						<description>Voucher Value</description>
						<modifiers optional="true" />
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>
		</typegroup>
		<typegroup name="cronjobs">
			<itemtype code="EmployeeIdentificationJob" jaloclass="br.hering.core.jobs.EmployeeIdentificationJob" extends="CronJob"/>
			<itemtype code="FixBoletoNotAuthorizedJob"	jaloclass="br.hering.core.jobs.FixBoletoNotAuthorizedJob" extends="CronJob"/>
			<itemtype code="FixTelephoneDDDMigrationJob" jaloclass="br.hering.core.jobs.FixTelephoneDDDMigrationJob" extends="CronJob"/>
			<itemtype code="ProductCarouselCleanupJob" jaloclass="br.hering.core.jobs.ProductCarouselCleanupJob" extends="CronJob"/>
			<itemtype code="ProductCarouselAddProductsJob" jaloclass="br.hering.core.jobs.ProductCarouselAddProductsJob" extends="CronJob">
				<attributes>
					<attribute qualifier="baseStore" type="BaseStore">
						<description>The base store to run against</description>
						<persistence type="property" />
						<modifiers read="true" write="true" />
					</attribute>
					<attribute qualifier="cmsSite" type="CMSSite">
						<description>The cms site to run against</description>
						<persistence type="property" />
						<modifiers read="true" write="true" />
					</attribute>
				</attributes>
			</itemtype>
			<itemtype code="ControlProductVisibilityJob" jaloclass="br.hering.core.jobs.ControlProductVisibilityJob" extends="CronJob">
				<attributes>
					<attribute qualifier="productCatalogVersions" type="CatalogVersionCollection">
						<description>List of catalog versions to search the products to run against</description>
						<modifiers optional="false"/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="numberOfRows" type="int">
						<description>Result set paging</description>
						<modifiers optional="false"/>
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>
			<itemtype	code="MenuGeneratorJob" 
							extends="CronJob"
							jaloclass="br.hering.core.jobs.MenuGeneratorJob">
					<attributes>
						<attribute qualifier="baseStore" type="BaseStore">
							<description>The base store to run against</description>
							<persistence type="property" />
							<modifiers read="true" write="true" optional="false"/>
						</attribute>
						<attribute qualifier="cmsSite" type="CMSSite">
							<description>The cms site to run against</description>
							<persistence type="property" />
							<modifiers read="true" write="true" optional="false"/>
						</attribute>
						<attribute autocreate="true" qualifier="menuDefinitions" type="MenuDefinitionCollection">
		               <modifiers read="true" write="true" search="false" removable="false" optional="true" partof="true"/>
		               <persistence type="property"/>
		            </attribute>						
					</attributes>
			</itemtype>
		</typegroup>
		<typegroup name="Restrictions">
			<itemtype code="UrlPatternRestriction" jaloclass="br.hering.core.jalo.restrictions.UrlPatternRestriction" extends="AbstractRestriction" autocreate="true" generate="true">
				<attributes>
					<attribute qualifier="urlPatterns" type="StringCollection">
						<persistence type="property" />
						<modifiers optional="false"/>
					</attribute>
					<attribute qualifier="matchAllPatterns" type="boolean">
						<persistence type="property" />
						<modifiers optional="false"/>
						<defaultvalue>java.lang.Boolean.TRUE</defaultvalue>
					</attribute>
				</attributes>
			</itemtype>
		</typegroup>
		<typegroup name="CMSComponents">
			<!-- CMS components -->
			<itemtype code="ProductCarouselComponent" autocreate="false" generate="false">
				<description>Extends existing type with additional attributes.</description>
				<attributes>
					<attribute qualifier="onlyNewArrivals" type="boolean">
						<persistence type="property" />
						<modifiers optional="false" />
						<defaultvalue>Boolean.FALSE</defaultvalue>
					</attribute>
					<attribute qualifier="onlyBestSellers" type="boolean">
						<persistence type="property" />
						<modifiers optional="false" />
						<defaultvalue>Boolean.FALSE</defaultvalue>
					</attribute>
					<attribute qualifier="carouselResultsSize" type="int">
						<persistence type="property" />
						<modifiers optional="true" />
					</attribute>
				</attributes>
			</itemtype>
		</typegroup>
	
		<typegroup>
			<itemtype	code="MenuDefinition"
	      				extends="GenericItem"
	      				jaloclass="br.hering.core.jalo.MenuDefinition"
	      				autocreate="true"
	      				generate="true">
		      	<description>Url for a given menu</description>
		         <deployment table="MenuDefinitions" typecode="6666"/>
		         <attributes>
		         	<attribute qualifier="name" type="java.lang.String">
		         		<modifiers read="true" write="true" search="true" optional="false" />
		         		<persistence type="property" />
		         		<description>Menu key - The phrase that appears at the headline</description>
		         	</attribute>
		         	<attribute qualifier="query" type="java.lang.String">
			         	<modifiers read="true" write="true" search="true" optional="false" />
			         	<persistence type="property"/>
			         	<description>Url value - the query that will be performed</description>
			         </attribute>
						<attribute qualifier="notAllowedCategories" type="StringCollection">
							<persistence type="property" />
						</attribute>
						<attribute qualifier="customCategories" type="String2StringMapType">
							<persistence type="property" />
						</attribute>
	          </attributes>
		      </itemtype>
			</typegroup>			
	</itemtypes>
</items>