<items xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="items.xsd">

	<collectiontypes>
		<collectiontype code="GenderList" elementtype="Gender" autocreate="true" generate="true" type="list"/>
		<collectiontype code="SwatchColorSet" elementtype="SwatchColorEnum" autocreate="true" generate="true" type="set"/>
		<collectiontype code="MeasurementsList" elementtype="ProductMeasurements" autocreate="true" generate="true" type="list"/>
		<collectiontype code="MenuDefinitionCollection" elementtype="MenuDefinition" autocreate="true" generate="false"/>
	</collectiontypes>

	<enumtypes>
      	<enumtype code="Gender" autocreate="false" generate="false">
			<value code="UNISEX"/>
			<value code="UNDEFINED"/>
		</enumtype>
		
		<enumtype code="OrderStatus" autocreate="false" generate="false">
			<value code="INVOICE" />
			<value code="DISPATCHED"/>
			<value code="PENDING_APPROVAL" />
			<value code="CAPTURE_PAYMENT" />
			<value code="WAITING_PAYMENT_NOTIFICATION" />
			<value code="ESTORNO" />
			<value code="RESERVED" />
			<value code="DELIVERED" />
			<value code="FRAUD_NOT_SENT" />
		</enumtype>
		
		<enumtype code="PaymentStatus" autocreate="false" generate="false">
			<value code="PENDING_APPROVAL" />
			<value code="WAITING_PAYMENT_NOTIFICATION" />
		</enumtype>
		
		<enumtype generate="true" code="SwatchColorEnum" autocreate="true" dynamic="true">
			<value code="BLACK"/>
			<value code="BLUE"/>
			<value code="BROWN"/>
			<value code="GREEN"/>
			<value code="GREY"/>
			<value code="ORANGE"/>
			<value code="PINK"/>
			<value code="PURPLE"/>
			<value code="RED"/>
			<value code="SILVER"/>
			<value code="WHITE"/>
			<value code="YELLOW"/>
		</enumtype>
		
		<enumtype code="ExportStatus" autocreate="false" generate="false">
		   <value code="NOTAUTHORIZED"/>
		</enumtype>
		
		<enumtype generate="true" code="TipoDeEndereco" autocreate="true" dynamic="true">
			<value code="RESIDENCIAL"/>
			<value code="COMERCIAL"/>
		</enumtype>
		
		<enumtype dynamic="true" generate="false" code="SolrPropertiesTypes" autocreate="false">
			<value code="semicolonDelimited" />
		</enumtype>
		
		<enumtype code="FraudStatus" autocreate="false" generate="false" dynamic="true">
			<value code="NOTSENT" />
		</enumtype>
	</enumtypes>
