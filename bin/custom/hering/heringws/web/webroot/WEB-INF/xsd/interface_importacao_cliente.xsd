<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	elementFormDefault="qualified" 
	attributeFormDefault="qualified" 
	targetNamespace="http://hering.fliegersoftware.de/interface/soap/" 
	xmlns:h="http://hering.fliegersoftware.de/interface/soap/">
  <xs:element name="importacaoClienteRequest">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="h:cliente"/>
        <xs:element maxOccurs="unbounded" ref="h:endereco"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="cliente">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="h:codigoCliente"/>
        <xs:element ref="h:nome"/>
        <xs:element ref="h:codigoSite"/>
        <xs:element ref="h:pfPj"/> 
        <xs:element ref="h:rgIe"/> 
        <xs:element ref="h:cpfCgc"/> 
        <xs:element ref="h:ddd"/>  
        <xs:element ref="h:telefone"/> 
        <xs:element ref="h:aniversario"/> 
        <xs:element ref="h:dataCadastramento"/> 
        <xs:element ref="h:sexo"/>
        <xs:element ref="h:email"/>
        <xs:element ref="h:senha" minOccurs="0"/>
        <xs:element ref="h:newsletter"/> 
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="nome" type="xs:string"/>
  <xs:element name="codigoSite" type="xs:string"/>
  <xs:element name="pfPj" type="xs:boolean"/>
  <xs:element name="rgIe" type="xs:string"/>
  <xs:element name="cpfCgc" type="xs:string"/>
  <xs:element name="aniversario" type="xs:string"/>
  <xs:element name="dataCadastramento" type="xs:string"/>
  <xs:element name="sexo" type="xs:string"/>
  <xs:element name="email" type="xs:string"/>
  <xs:element name="senha" type="xs:string"/>
  <xs:element name="newsletter" type="xs:boolean"/>
  <xs:element name="endereco">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="h:codigoEndereco"/>
        <xs:element ref="h:descricaoEndereco"/>
        <xs:element ref="h:pais"/>
        <xs:element ref="h:cep"/>
        <xs:element ref="h:bairro"/>
        <xs:element ref="h:cidade"/>
        <xs:element ref="h:complemento" minOccurs="0" />
        <xs:element ref="h:numero"/>
        <xs:element ref="h:logradouro"/>
        <xs:element ref="h:uf"/>
        <xs:element ref="h:enderecoPrincipal"/>
        <xs:element ref="h:tipoEndereco"/>
        <xs:element ref="h:estadoExterior" minOccurs="0" />
        <xs:element ref="h:ddd"/>
        <xs:element ref="h:telefone"/>
        <xs:element ref="h:dddCelular" minOccurs="0"/>
        <xs:element ref="h:celular" minOccurs="0"/>
        <xs:element ref="h:codigoCliente"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="codigoEndereco" type="xs:string"/>
  <xs:element name="descricaoEndereco" type="xs:string"/>
  <xs:element name="pais" type="xs:string"/>
  <xs:element name="cep" type="xs:string"/>
  <xs:element name="bairro" type="xs:string"/>
  <xs:element name="cidade" type="xs:string"/>
  <xs:element name="complemento" type="xs:string"/>
  <xs:element name="numero" type="xs:string"/>
  <xs:element name="logradouro" type="xs:string"/>
  <xs:element name="uf" type="xs:string"/>
  <xs:element name="enderecoPrincipal" type="xs:boolean"/>
  <xs:element name="tipoEndereco" type="xs:string"/>
  <xs:element name="estadoExterior" type="xs:string"/>
  <xs:element name="dddCelular" type="xs:string"/>
  <xs:element name="celular" type="xs:string"/>
  <xs:element name="codigoCliente" type="xs:string"/>
  <xs:element name="ddd" type="xs:string"/>
  <xs:element name="telefone" type="xs:string"/>
</xs:schema>
