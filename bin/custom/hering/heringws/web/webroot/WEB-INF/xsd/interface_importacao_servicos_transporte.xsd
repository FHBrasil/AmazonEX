<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	elementFormDefault="qualified" 
	attributeFormDefault="qualified" 
	targetNamespace="http://hering.fliegersoftware.de/interface/soap/" 
	xmlns:h="http://hering.fliegersoftware.de/interface/soap/">
  <xs:element name="importarServicosTransporteRequest">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="h:servicoTransporte"/>
        <xs:element maxOccurs="unbounded" ref="h:frete"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="servicoTransporte">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="codigoSite" type="xs:string"/>
        <xs:element name="codigoTransportadora" type="xs:string"/>
        <xs:element name="nomeTransportadora" type="xs:string"/>
        <xs:element name="formaEnvio" type="xs:string"/>
        <xs:element name="dataInicialVigencia" type="xs:string"/>
        <xs:element name="dataFinalVigencia" type="xs:string"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="frete">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="cepInicial" type="xs:string"/>
        <xs:element name="cepFinal" type="xs:string"/>
        <xs:element name="faixaPesoInicial" type="xs:int"/>
        <xs:element name="faixaPesoFinal" type="xs:int"/>
        <xs:element name="cidade" type="xs:string" minOccurs="0"/>
        <xs:element name="uf" type="xs:string" minOccurs="0"/>
        <xs:element name="prazoEntrega" type="xs:int"/>
        <xs:element name="valorPeso" type="xs:double"/>
        <xs:element name="valorPedagio" type="xs:double"/>
        <xs:element name="valorPercentual" type="xs:double"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>
