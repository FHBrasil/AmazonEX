<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	elementFormDefault="qualified" 
	attributeFormDefault="qualified" 
	targetNamespace="http://hering.fliegersoftware.de/interface/soap/" 
	xmlns:h="http://hering.fliegersoftware.de/interface/soap/">

	<xs:element name="resultadoTransacaoRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element maxOccurs="unbounded" ref="h:resultado"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="resultado">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="h:transacao"/>
				<xs:element ref="h:chave"/>
				<xs:element ref="h:mensagem"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="transacao">
		<xs:annotation>
			<xs:documentation>Processo de integração</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="25"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="chave">
		<xs:annotation>
			<xs:documentation>Campo chave para identificação</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="25"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="mensagem" type="xs:string">
		<xs:annotation>
			<xs:documentation>Mensagem de erro ou alerta</xs:documentation>
		</xs:annotation>
	</xs:element>
</xs:schema>
