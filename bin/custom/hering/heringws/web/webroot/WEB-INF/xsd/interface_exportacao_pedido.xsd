<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	elementFormDefault="qualified" 
	attributeFormDefault="qualified" 
	targetNamespace="http://hering.fliegersoftware.de/interface/soap/" 
	xmlns:h="http://hering.fliegersoftware.de/interface/soap/">
	<xs:element name="importarPedidoRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="h:dadosPedido"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="dadosPedido">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="h:pedido"/>
				<xs:element maxOccurs="unbounded" ref="h:pagamento"/>
				<xs:element maxOccurs="unbounded" ref="h:itens"/>
				<xs:element ref="h:cliente"/>
				<xs:element maxOccurs="unbounded" ref="h:endereco"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="pedido">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="h:codigoSite"/>
				<xs:element ref="h:codigoCliente"/>
				<xs:element ref="h:codigoEnderecoEntrega"/>
				<xs:element ref="h:codigoEnderecoCobranca"/>
				<xs:element ref="h:codigoPedido"/>
				<xs:element ref="h:data"/>
				<xs:element ref="h:tipo"/>
				<xs:element ref="h:quantidadeTotal"/>
				<xs:element ref="h:valorTotal"/>
				<xs:element ref="h:desconto"/>
				<xs:element ref="h:formaEntrega"/>
				<xs:element ref="h:transportadora"/>
				<xs:element ref="h:peso"/>
				<xs:element ref="h:prazoMaximoEntrega"/>
				<xs:element ref="h:valorFrete"/>
				<xs:element ref="h:mensagemCartao" minOccurs="0"/>
				<xs:element ref="h:codigoDesconto" minOccurs="0"/>
				<xs:element ref="h:dataHoraFinalizacaoPedido"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="codigoEnderecoEntrega">
		<xs:annotation>
			<xs:documentation>FK - código do endereço do cliente</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:minLength value="20"/>
				<xs:maxLength value="20"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="codigoEnderecoCobranca">
		<xs:annotation>
			<xs:documentation>FK - código do endereço do cliente hybris</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:minLength value="20"/>
				<xs:maxLength value="20"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="data">
		<xs:annotation>
			<xs:documentation>Data do pedido</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:minLength value="19"/>
				<xs:maxLength value="19"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="tipo">
		<xs:annotation>
			<xs:documentation>Tipo do pedido de Gerado</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="4"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="quantidadeTotal" type="xs:int">
		<xs:annotation>
			<xs:documentation>Quantidade total do Pedido</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="valorTotal">
		<xs:annotation>
			<xs:documentation>Valor total do pedido + frete</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:decimal">
				<xs:totalDigits value="15"/>
				<xs:fractionDigits value="2"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="formaEntrega">
		<xs:annotation>
			<xs:documentation>Forma da entrega do pedido</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="25"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="transportadora">
		<xs:annotation>
			<xs:documentation>Transportadora do Pedido</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="25"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="peso">
		<xs:annotation>
			<xs:documentation>Peso total do pedido</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:decimal">
				<xs:totalDigits value="12"/>
				<xs:fractionDigits value="3"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="prazoMaximoEntrega" type="xs:int">
		<xs:annotation>
			<xs:documentation>Prazo máximo de entrega do pedido</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="valorFrete">
		<xs:annotation>
			<xs:documentation>Valor do frete cobrado</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:decimal">
				<xs:totalDigits value="15"/>
				<xs:fractionDigits value="2"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="mensagemCartao">
		<xs:annotation>
			<xs:documentation>Mensagem para o cartão de Crédito</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="300"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="codigoDesconto">
		<xs:annotation>
			<xs:documentation>Código do desconto utilizado</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="50"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="dataHoraFinalizacaoPedido">
		<xs:annotation>
			<xs:documentation>Data da finalização do pedido</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:minLength value="19"/>
				<xs:maxLength value="19"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="pagamento">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="h:codigoSite"/>
				<xs:element ref="h:codigoPedido"/>
				<xs:element ref="h:parcela"/>
				<xs:element ref="h:tipoPagamento"/>
				<xs:element ref="h:valor"/>
				<xs:element ref="h:vencimento"/>
				<xs:element ref="h:numeroTitulo"/>
				<xs:element ref="h:moeda"/>
				<xs:element ref="h:valorMoeda"/>
				<xs:element ref="h:codigoAdministradora"/>
				<xs:element ref="h:numeroAprovacaoCartao"/>
				<xs:element ref="h:parcelasCartao"/>
				<xs:element ref="h:cotacao"/>
				<xs:element ref="h:capturado"/>
				<xs:element ref="h:codigoAntifraude"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="parcela" type="xs:int">
		<xs:annotation>
			<xs:documentation>PK -  da parcela do pagamento total</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="tipoPagamento">
		<xs:annotation>
			<xs:documentation>Meio de pagamento da Parcela</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="1"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="valor">
		<xs:annotation>
			<xs:documentation>Valor da Parcela</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:decimal">
				<xs:totalDigits value="15"/>
				<xs:fractionDigits value="2"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="vencimento">
		<xs:annotation>
			<xs:documentation>Vencimento da Parcela ou Boleto</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:minLength value="19"/>
				<xs:maxLength value="19"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="numeroTitulo">
		<xs:annotation>
			<xs:documentation>Número do boleto ou cartão</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="15"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="moeda">
		<xs:annotation>
			<xs:documentation>Moeda utilizada</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="6"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="valorMoeda">
		<xs:annotation>
			<xs:documentation>Valor na moeda utilizada</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:decimal">
				<xs:totalDigits value="15"/>
				<xs:fractionDigits value="2"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="codigoAdministradora" type="xs:int">
		<xs:annotation>
			<xs:documentation>Código da administradora do cartão</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="numeroAprovacaoCartao">
		<xs:annotation>
			<xs:documentation>Número da aprovação do cartão(NSU)</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="15"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="parcelasCartao" type="xs:int">
		<xs:annotation>
			<xs:documentation>Quantidade de parcelas</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="cotacao">
		<xs:annotation>
			<xs:documentation>Cotação em real na venda de moeda estrangeira</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:decimal">
				<xs:totalDigits value="19"/>
				<xs:fractionDigits value="5"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="capturado" type="xs:int">
		<xs:annotation>
			<xs:documentation>Aprovação na ferramenta de antifraude</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="codigoAntifraude">
		<xs:annotation>
			<xs:documentation>Ferramenta antifraude Utilizada</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="25"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="itens">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="h:codigoSite"/>
				<xs:element ref="h:codigoPedido"/>
				<xs:element ref="h:codigoProduto"/>
				<xs:element ref="h:corProduto"/>
				<xs:element ref="h:item"/>
				<xs:element ref="h:sku"/>
				<xs:element ref="h:quantidade"/>
				<xs:element ref="h:precoLiquido"/>
				<xs:element ref="h:desconto"/>
				<xs:element ref="h:status"/>
				<xs:element ref="h:origemEstoque" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="codigoProduto">
		<xs:annotation>
			<xs:documentation>FK - Código do produto</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="12"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="corProduto">
		<xs:annotation>
			<xs:documentation>Código da cor</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="10"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="item">
		<xs:annotation>
			<xs:documentation>PK  Item do pedido</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="10"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="sku">
		<xs:annotation>
			<xs:documentation>FK - Código único do produto</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="30"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="quantidade" type="xs:int">
		<xs:annotation>
			<xs:documentation>Quantidade do item no pedido</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="precoLiquido">
		<xs:annotation>
			<xs:documentation>Preço Liquido já retirando os desconto</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:decimal">
				<xs:totalDigits value="15"/>
				<xs:fractionDigits value="2"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="status" type="xs:int">
		<xs:annotation>
			<xs:documentation>Status</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="origemEstoque">
		<xs:annotation>
			<xs:documentation>Origem do estoque</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="25"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="cliente">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="h:codigoCliente"/>
				<xs:element ref="h:nome"/>
				<xs:element ref="h:codigoSite"/>
				<xs:element ref="h:pfPj"/>
				<xs:element ref="h:rgIe"/>
				<xs:element ref="h:cpfCgc"/>
				<xs:element ref="h:ddd"/>
				<xs:element ref="h:telefone"/>
				<xs:element ref="h:aniversario"/>
				<xs:element ref="h:dataCadastramento"/>
				<xs:element ref="h:sexo"/>
				<xs:element ref="h:email"/>
				<xs:element ref="h:senha" minOccurs="0"/>
				<xs:element ref="h:newsletter"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="nome">
		<xs:annotation>
			<xs:documentation>Nome completo do Cliente</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="40"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="pfPj" type="xs:boolean">
		<xs:annotation>
			<xs:documentation>Indica se é Pessoa Física</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="rgIe">
		<xs:annotation>
			<xs:documentation>Nº do Rg ou Inscrição Estadual</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="19"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="cpfCgc">
		<xs:annotation>
			<xs:documentation>CPF ou CNPJ dependendo do tipo de cliente</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="19"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="aniversario">
		<xs:annotation>
			<xs:documentation>Data de nascimento</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:minLength value="19"/>
				<xs:maxLength value="19"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="dataCadastramento">
		<xs:annotation>
			<xs:documentation>Data do cadastramento no Site</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:minLength value="19"/>
				<xs:maxLength value="19"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="sexo">
		<xs:annotation>
			<xs:documentation>Sexo do Cliente</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="1"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="email">
		<xs:annotation>
			<xs:documentation>E-mail do cadastro</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="senha">
		<xs:annotation>
			<xs:documentation>Senha</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="250"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="newsletter" type="xs:boolean">
		<xs:annotation>
			<xs:documentation>Indica se recebera newsletter</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="endereco">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="h:codigoEndereco"/>
				<xs:element ref="h:descricaoEndereco"/>
				<xs:element ref="h:pais"/>
				<xs:element ref="h:cep"/>
				<xs:element ref="h:bairro"/>
				<xs:element ref="h:cidade"/>
				<xs:element ref="h:complemento" minOccurs="0"/>
				<xs:element ref="h:numero"/>
				<xs:element ref="h:logradouro"/>
				<xs:element ref="h:uf"/>
				<xs:element ref="h:enderecoPrincipal"/>
				<xs:element ref="h:tipoEndereco"/>
				<xs:element ref="h:estadoExterior" minOccurs="0"/>
				<xs:element ref="h:ddd"/>
				<xs:element ref="h:telefone"/>
				<xs:element ref="h:dddCelular" minOccurs="0"/>
				<xs:element ref="h:celular" minOccurs="0"/>
				<xs:element ref="h:codigoCliente"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="codigoEndereco">
		<xs:annotation>
			<xs:documentation>PK- identificação relacionada com o Pedido</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="20"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="descricaoEndereco">
		<xs:annotation>
			<xs:documentation>Nome de quem receberá o Pedido</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="50"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="pais">
		<xs:annotation>
			<xs:documentation>País</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="35"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="cep">
		<xs:annotation>
			<xs:documentation>Código Postal</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="9"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="bairro">
		<xs:annotation>
			<xs:documentation>Bairro</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="25"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="cidade">
		<xs:annotation>
			<xs:documentation>Cidade</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="35"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="complemento">
		<xs:annotation>
			<xs:documentation>Complemento do endereço</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="20"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="numero">
		<xs:annotation>
			<xs:documentation>Número do endereço</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="10"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="logradouro">
		<xs:annotation>
			<xs:documentation>Endereço sem números</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="40"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="uf">
		<xs:annotation>
			<xs:documentation>Estado da federação</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="2"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="enderecoPrincipal" type="xs:boolean">
