<items 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
			xsi:noNamespaceSchemaLocation="items.xsd">
	
	<collectiontypes>
		<collectiontype code="OrderStatusSet" elementtype="OrderStatus" autocreate="true"  type="set"/>
	</collectiontypes>
	
	<itemtypes>
			<itemtype code="ConsignmentProcess" autocreate="false" generate="false" >
				<attributes>
					<attribute qualifier="done" type="boolean">
						<description>Mark process as done</description>
						<modifiers optional="false" initial="false"/>
						<persistence type="property" />
						<defaultvalue>Boolean.FALSE</defaultvalue>					
					</attribute>			
					
					<attribute qualifier="waitingForConsignment" type="boolean">
						<description>Mark that process is waiting for consignment</description>
						<modifiers optional="false" initial="false"/>
						<persistence type="property" />
						<defaultvalue>Boolean.FALSE</defaultvalue>					
					</attribute>			
					
					<attribute qualifier="warehouseConsignmentState" type="WarehouseConsignmentState">
						<description>State of warehouse process.</description>
						<persistence type="property" />
						<modifiers initial="false" optional="true" />
					</attribute>
						
				</attributes>
			</itemtype>

			<itemtype code="Consignment" autocreate="false" generate="false">
				<attributes>
					<attribute type="OrderInvoice" qualifier="invoice">
						<persistence type="property" />
					</attribute>
				</attributes>
			</itemtype>
			
			<itemtype code="OrderInvoice">
				<deployment table="OrderInvoice" typecode="10003"/>
				<attributes>
					<attribute type="java.lang.String" qualifier="key">
						<persistence type="property" />
					</attribute>
					<attribute type="java.lang.String" qualifier="number">
						<persistence type="property" />
					</attribute>
					<attribute type="java.util.Date" qualifier="issueDate">
						<persistence type="property" />
					</attribute>
				</attributes>
			</itemtype>
			
			<itemtype code="ExportOrdersJob" extends="CronJob" jaloclass="br.hering.fulfilmentprocess.jobs.ExportOrdersJob">
				<attributes>
					<attribute qualifier="fromDate" type="java.util.Date">
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="toDate" type="java.util.Date">
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="statusCreditCardFilter" type="OrderStatusSet">
						<persistence type="property"/>
						<modifiers optional="false"/>
					</attribute>
					<attribute qualifier="statusBoletoFilter" type="OrderStatusSet">
						<persistence type="property"/>
						<modifiers optional="false"/>
					</attribute>
					<attribute qualifier="statusVoucherFilter" type="OrderStatusSet">
						<persistence type="property"/>
						<modifiers optional="false"/>
					</attribute>
				</attributes>
			</itemtype>
			
			<itemtype code="TelephoneCellphoneReplaceCharJob" extends="CronJob" jaloclass="br.hering.fulfilmentprocess.jobs.TelephoneCellphoneReplaceCharJob">
				
			</itemtype>
			
			<itemtype code="ClearsaleOrderResendJob" extends="CronJob" jaloclass="br.hering.fulfilmentprocess.jobs.ClearsaleOrderResendJob">
				
			</itemtype>
			
			<itemtype code="ProductStatusUpdateJob" jaloclass="br.hering.fulfilmentprocess.jobs.ProductStatusUpdateJob" extends="CronJob"/>
			
			<itemtype code="ImportProductPicturesJob" extends="CronJob" jaloclass="br.hering.fulfilmentprocess.jobs.ImportProductPicturesJob">
				
			</itemtype>
			
			<itemtype code="StockReserveRollBackJob" extends="CronJob" jaloclass="br.hering.fulfilmentprocess.jobs.StockReserveRollBackJob">
				<attributes>
					<attribute qualifier="fromDate" type="java.util.Date">
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="toDate" type="java.util.Date">
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="statusFilter" type="OrderStatusSet">
						<persistence type="property"/>
						<modifiers optional="false"/>
					</attribute>
					<attribute qualifier="updateAllToNotAuth" type="boolean">
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>
			
	</itemtypes>
</items>