	<parameter name="DATE_END" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT {hs.code} AS sku, COUNT({hs.code}) AS sku_quantity
     , {c.id} AS catalog
FROM {Notifyme AS n
      JOIN HeringSizeVariantProduct AS hs ON {hs.code} = {n.code}
      JOIN Catalog AS c ON {c.PK} = {hs.catalog}
      JOIN HeringStyleVariantProduct AS ht ON {ht.PK} = {hs.baseProduct} AND {ht.catalog} = {c.PK}
     }
WHERE {n.notified} is null
      AND {n.created} BETWEEN $P{DATE_INI} AND $P{DATE_END}
GROUP BY {hs.code}, {c.id}]]>
	</queryString>
	<field name="sku" class="java.lang.String"/>
	<field name="sku_quantity" class="java.lang.Long"/>
	<field name="catalog" class="java.lang.String"/>
