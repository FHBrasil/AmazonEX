<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="WishlistProductsDiseredByReceived" language="groovy" pageWidth="792" pageHeight="612" orientation="Landscape" columnWidth="752" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<queryString>
		<![CDATA[SELECT TRUNC({we.addedDate}) as addedToWishlistDate
     , COUNT({we.received}) AS received
     , COUNT({we.desired}) AS desired
FROM {WishList2Entry AS we}
WHERE TRUNC({we.addedDate}) BETWEEN TRUNC(SYSDATE) - 30 AND TRUNC(SYSDATE)
GROUP BY TRUNC({we.addedDate})]]>
	</queryString>
	<field name="addedToWishlistDate" class="java.sql.Date"/>
	<field name="received" class="java.lang.Long"/>
	<field name="desired" class="java.lang.Long"/>
