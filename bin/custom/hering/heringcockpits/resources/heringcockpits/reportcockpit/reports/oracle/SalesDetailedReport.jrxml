	<queryString>
		<![CDATA[SELECT distinct {o.code} AS orderId
     , {pte.time} AS confirmationDate
     , {u.name} AS customerName
     , CONCAT({da.firstName}, CONCAT(' ', {da.lastName})) AS deliveredTo
     , {o.totalPrice} AS paidPrice
     , {o.totalDiscounts} AS totalDiscount
     , {o.totalPrice} - {o.totalDiscounts} + {o.totalTax} AS totalOrderValue
     , {os.code} AS orderStatus
     , {ps.code} AS paymentStauts
     , {u.uid} AS customerCode
     , {pm.code} AS paymentMode
     , CONCAT(COALESCE({da.town}, ' '),
       CONCAT(', ', CONCAT(COALESCE({da.district}, ' '),
       CONCAT(' - ', CONCAT(COALESCE({da.streetName}, ' '),
       CONCAT(' ', COALESCE({da.building}, ' ')))))))  AS deliveryAddress
     , {bpi.nossoNumero} AS paymentSlip
FROM {Order AS o
	  JOIN User AS u ON {u.pk} = {o.user}
      JOIN Address AS da ON {da.pk} = {o.deliveryAddress} AND {da.shippingAddress} = 1
      JOIN PaymentMode AS pm ON {pm.pk} = {o.paymentMode}
      JOIN PaymentStatus AS ps ON {ps.pk} = {o.paymentStatus}
      JOIN PaymentInfo AS pi ON {pi.pk} = {o.paymentInfo}
      LEFT OUTER JOIN BoletoPaymentInfo AS bpi ON {bpi.pk} = {o.paymentInfo}
      JOIN OrderStatus AS os ON {os.pk} = {o.status}
      JOIN PaymentTransaction AS pt ON {pt.order} = {o.pk}
      JOIN PaymentTransactionEntry AS pte ON {pte.paymentTransaction} = {pt.pk}
}
WHERE {pte.time} BETWEEN (SYSDATE - 5) AND SYSDATE]]>
	</queryString>
