	<queryString>
		<![CDATA[SELECT {o.code} AS orderId
     , {o.date} AS orderDate
     , {os.code} AS orderStatus
     , {pm.code} AS paymentMode
     , {o.totalPrice} AS pricePaid
     , {o.totalPrice} + {o.totalDiscounts} AS priceIssued
     , {u.name} AS customerName
     , {u.uid} AS customerEmail
     , {b.uid} AS storeUid
     , CONCAT({a.firstname}, CONCAT(' ', {a.lastname})) AS receiver
     , {dm.name[pt]}
FROM {
      Order AS o
      LEFT OUTER JOIN PaymentStatus AS p ON {p.pk} = {o.paymentStatus}
      LEFT OUTER JOIN OrderStatus AS os ON {os.pk} = {o.status}
      LEFT OUTER JOIN User AS u ON {u.pk} = {o.user}
      LEFT OUTER JOIN ClearSaleFraudReport AS cs ON {cs.order} = {o.pk}
      LEFT OUTER JOIN OrderProcess AS op ON {op.order} = {o.pk}
      LEFT OUTER JOIN ProcessState AS ps ON {ps.pk} = {op.state}
      LEFT OUTER JOIN PaymentMode AS pm ON {pm.pk} = {o.paymentMode}
      LEFT OUTER JOIN ExportStatus AS es ON {es.pk} = {o.exportStatus}
      INNER JOIN BaseSite AS b ON {b.pk} = {o.site}
      LEFT OUTER JOIN Address AS a ON {a.pk} = {o.deliveryAddress}
      LEFT OUTER JOIN DeliveryMode AS dm ON {dm.pk} = {o.deliveryMode}
     }
WHERE {o.date} BETWEEN TO_DATE$P{DATE_INI} AND $P{DATE_END}
      AND ({os.code} != 'CANCELLED' AND {os.code} != 'DISPATCHED'
                AND {os.code} != 'INVOICE')
      AND {op.processDefinitionName} != 'orderConfirmationEmailProcess'
      AND ({es.code} != 'EXPORTED' OR {o.exportStatus} is null)
      AND {o.totalDiscounts} > 0
ORDER BY {o.code}]]>
	</queryString>
