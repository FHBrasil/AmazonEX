<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="NewsletterSubscribersReport_Subreport" language="groovy" pageWidth="792" pageHeight="612" orientation="Landscape" columnWidth="752" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<queryString>
		<![CDATA[SELECT x.customerQuantity, x.dateRef
FROM(
   {{ SELECT count(distinct {n.email}) AS customerQuantity
           , to_date(to_char(sysdate, 'yyyy-mm-dd'), 'yyyy-mm-dd') as dateRef
      FROM {NewsletterSubscriber AS n}
      WHERE {n.creationTime} < to_date(to_char(sysdate, 'yyyy-mm-dd'), 'yyyy-mm-dd')
      GROUP BY to_char(sysdate, 'yyyy-mm-dd') }}
   UNION
   {{ SELECT count(distinct {n.email}) AS customerQuantity
           , to_date(to_char(sysdate-1, 'yyyy-mm-dd'), 'yyyy-mm-dd') as dateRef
      FROM {NewsletterSubscriber AS n}
      WHERE {n.creationTime} < to_date(to_char(sysdate-1, 'yyyy-mm-dd'), 'yyyy-mm-dd')
      GROUP BY to_char(sysdate-1, 'yyyy-mm-dd') }}
   UNION
   {{ SELECT count(distinct {n.email}) AS customerQuantity
           , to_date(to_char(sysdate-2, 'yyyy-mm-dd'), 'yyyy-mm-dd') as dateRef
      FROM {NewsletterSubscriber AS n}
      WHERE {n.creationTime} < to_date(to_char(sysdate-2, 'yyyy-mm-dd'), 'yyyy-mm-dd')
      GROUP BY to_char(sysdate-2, 'yyyy-mm-dd') }}
) x]]>
	</queryString>
	<field name="customerQuantity" class="java.lang.Integer"/>
	<field name="dateRef" class="java.sql.Date"/>
	<background>
		<band splitType="Stretch"/>
	</background>
	<pageHeader>
