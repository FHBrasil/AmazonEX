			<staticText>
				<reportElement stretchType="RelativeToBandHeight" x="0" y="0" width="752" height="32" isPrintWhenDetailOverflows="true"/>
				<box topPadding="0" leftPadding="0" bottomPadding="0" rightPadding="0">
					<pen lineWidth="1.0"/>
					<topPen lineWidth="0.0"/>
					<leftPen lineWidth="0.0"/>
					<bottomPen lineWidth="0.0"/>
					<rightPen lineWidth="0.0"/>
				</box>
				<textElement textAlignment="Center" verticalAlignment="Middle">
					<font size="26" isBold="true"/>
				</textElement>
				<text><![CDATA[Assinantes Newsletter]]></text>
			</staticText>
		</band>
	</pageHeader>
	<summary>
		<band height="212" splitType="Stretch">
			<lineChart>
				<chart>
					<reportElement x="0" y="0" width="752" height="212"/>
					<chartTitle/>
					<chartSubtitle/>
					<chartLegend/>
				</chart>
				<categoryDataset>
					<categorySeries>
						<seriesExpression><![CDATA["Subscribers"]]></seriesExpression>
						<categoryExpression><![CDATA[$F{dateRef}]]></categoryExpression>
						<valueExpression><![CDATA[$F{customerQuantity}]]></valueExpression>
					</categorySeries>
				</categoryDataset>
				<linePlot>
					<plot/>
					<categoryAxisFormat>
						<axisFormat/>
					</categoryAxisFormat>
					<valueAxisFormat>
						<axisFormat/>
					</valueAxisFormat>
				</linePlot>
			</lineChart>
		</band>
	</summary>
</jasperReport>
