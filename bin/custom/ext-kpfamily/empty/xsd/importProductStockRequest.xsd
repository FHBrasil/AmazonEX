<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="ARTICLE_ITEM">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ARTICLE_ITEM_ID"/>
        <xs:element ref="QUANTITY"/>
        <xs:element ref="DELIVERY_DATE"/>
        <xs:element ref="MIN_STOCK_QTY"/>
        <xs:element ref="ACTIVE"/>
        <xs:element ref="EAN"/>
        <xs:element ref="OPENSUPPLORDERS"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ARTICLE_ITEM_ID" type="xs:string"/>
  <xs:element name="QUANTITY" type="xs:int"/>
  <xs:element name="DELIVERY_DATE" type="xs:date"/>
  <xs:element name="MIN_STOCK_QTY" type="xs:int"/>
  <xs:element name="ACTIVE" type="xs:boolean"/>
  <xs:element name="EAN" type="xs:string"/>
  <xs:element name="OPENSUPPLORDERS" type="xs:int"/>
</xs:schema>
