#Antes de realizar a IMPORTAÇÃO os seguintes passos devem ser realizados: 
#- trocar o INSERT por INSERT_UPDATE
#- mudar a coluna product(code) para productId(code)[unique=true]
#- substituir todos os . (ponto) por , (virgula)

$catalog=babyartikel
$version=Staged

INSERT PriceRow;product(code);price;unit(code[unique=true,default=pieces]);currency(isocode)[unique=true,default=EUR];minqtd[default=1];unitFactor[default=1];net[default=false]
"#% impex.exportItems(""SELECT {PR:pk} FROM {PriceRow as PR}, {KPProduct as P}, {CatalogVersion as CV}, {Catalog as C} WHERE {PR:product}={P:PK} AND {P:catalogversion}={CV:PK} AND {CV:catalog}={C:PK} AND {C:id}='$catalog' AND {CV:version}='$version'"", Collections.EMPTY_MAP, Collections.singletonList( Item.class ), true, true, -1, -1  );"