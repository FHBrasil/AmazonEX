# ---- Export Script -----------------------------------------------------------
"#% impex.setTargetFile(""PriceHistoryEntry.csv"");"
insert PriceHistoryEntry;&priceHistoryEntryPK;product(&productSellingPriceHistoryRelationPK);date[dateformat=dd.MM.yyyy hh:mm:ss];value;
"#% impex.exportItems(""PriceHistoryEntry"", false);"
# 
"#% impex.setTargetFile(""PurchasePriceHistoryRelation.csv"");"
insert ProductPurchasePriceHistoryRelation;source(code);target(&priceHistoryEntryPK);
"#% impex.exportItems(""ProductPurchasePriceHistoryRelation"", false);"
# 
"#% impex.setTargetFile(""ProductSellingPriceHistoryRelation.csv"");"
insert ProductSellingPriceHistoryRelation;&productSellingPriceHistoryRelationPK;source(code);target(&priceHistoryEntryPK);
"#% impex.exportItems(""ProductSellingPriceHistoryRelation"", false);"
