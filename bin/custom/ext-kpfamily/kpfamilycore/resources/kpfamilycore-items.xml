<items xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="items.xsd">

	<collectiontypes>
		<collectiontype code="GenderList" elementtype="Gender" autocreate="true" generate="true" type="list"/>
		<collectiontype code="SwatchColorSet" elementtype="SwatchColorEnum" autocreate="true" generate="true" type="set"/>
	</collectiontypes>
	
	<enumtypes>
		<enumtype generate="true" code="SwatchColorEnum" autocreate="true" dynamic="true">
			<value code="BLACK"/>
			<value code="BLUE"/>
			<value code="BROWN"/>
			<value code="GREEN"/>
			<value code="GREY"/>
			<value code="ORANGE"/>
			<value code="PINK"/>
			<value code="PURPLE"/>
			<value code="RED"/>
			<value code="SILVER"/>
			<value code="WHITE"/>
			<value code="YELLOW"/>
		</enumtype>
		<enumtype generate="true" code="ArticleState" autocreate="true">
			<value code="CURRENT"/>
			<value code="DEPRECATED"/>
			<value code="OLD"/>
		</enumtype>
		<enumtype generate="true" code="ShippingMethod" autocreate="true">
			<value code="WAREHOUSE"/>
			<value code="DROPSHIPPING"/>
		</enumtype>
		<enumtype code="OrderStatus" autocreate="false" generate="true" dynamic="true">
			<value code="RETURNED"/>
			<value code="CHARGEBACK"/>
		</enumtype>
		 <enumtype code="OrderEntryStatus" autocreate="false" generate="true" dynamic="false">
			<value code="CHARGEBACK"/>
			<value code="SHIPPED"/>
			<value code="RETURNED"/>
			<value code="AWAITING_PAYMENT"/>
			<value code="AWAITING_SHIPPING"/>
			<value code="PROCESSING"/>
			<value code="NOT_AVAILABLE"/>
			<value code="DIRECT_SHIPMENT"/>
			<value code="DIRECT_SHIPMENT_PACKED"/>
			<value code="EXCHANGE"/>
		</enumtype>
	</enumtypes>
	
	<relations>
		<relation code="Product2ProductSupplierRelation" autocreate="true" generate="false" localized="false">
			<deployment table="Product2SupplierRel" typecode="10002" />
			<sourceElement type="BabyartikelProduct" cardinality="many" navigable="false" />
			<targetElement type="ProductSupplier" qualifier="suppliers" collectiontype="list" cardinality="many"/>
		</relation>
	</relations>

	<itemtypes>
		<typegroup name="Hybris">
			 <itemtype code="AbstractOrderEntry" autocreate="false" generate="false">
				<attributes>
		            <attribute qualifier="status" type="OrderEntryStatus">
						<modifiers />						
						<persistence type="property" />
					</attribute>
				</attributes>
			</itemtype>
			<itemtype code="OrderEntryStatus" generate="false" autocreate="false">
				<attributes>
					<attribute qualifier="externalCode" type="java.lang.String" generate="false">
						<modifiers/>
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>
			<itemtype code="StockLevel" generate="false" autocreate="false">
				<attributes>
					<attribute qualifier="minimumStockLevel" type="int">
						<modifiers/>
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>
		</typegroup>
		
		<typegroup name="Babyartikel">
			<itemtype code="BabyartikelProduct" extends="Product"
			          autocreate="true" generate="true" 
			          jaloclass="de.kpfamily.core.jalo.BabyartikelProduct">
				<description>Base babyartikel product extension that contains additional attributes.</description>
				<attributes>
					<attribute qualifier="shortDescription" type="localized:java.lang.String">
						<description> A short description of the product. </description>
						<persistence type="property">
							<columntype database="mysql">
								<value>text</value>
							</columntype>
						</persistence>
						<modifiers read="true" write="true" optional="true"/>
					</attribute>
					<attribute qualifier="articleState" type="ArticleState">
						<description>Defines the current state of the product within it's collection</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="articleStateInternalCode" type="java.lang.String">
						<description>Dynamic attribute to retrieve the article state internal code</description>
						<modifiers read="true" write="false" />
						<persistence type="dynamic" attributeHandler="articleStateInternalCodeHandler"/>
					</attribute>
					<attribute qualifier="shippingMethod" type="ShippingMethod">
						<description>Defines the way that the product is shipped</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="shippingMethodInternalCode" type="java.lang.String">
						<description>Dynamic attribute to retrieve the shipping method internal code</description>
						<modifiers read="true" write="false" />
						<persistence type="dynamic" attributeHandler="shippingMethodInternalCodeHandler"/>
					</attribute>
					<attribute qualifier="supplierPrice" type="double">
						<description>The price that the product was bought from the supplier</description>
						<persistence type="property"/>
						<modifiers />
					</attribute>
					<attribute qualifier="nextDeliveryDate" type="java.util.Date">
						<description>delivery date that comes from PIXI</description>
						<persistence type="property"/>
						<modifiers />
					</attribute>
					<attribute qualifier="weight" type="double">
						<description>Weight of the product, this attribute is dynamic to make it easier 
						to get the value from the classification system</description>
				    	<persistence type="dynamic"/>
				    	<modifiers write="false"/>
				  	</attribute>
				  	<attribute qualifier="length" type="double">
				  		<description>Length of the product, this attribute is dynamic to make it easier 
						to get the value from the classification system</description>
				    	<persistence type="dynamic"/>
				    	<modifiers write="false"/>
				  	</attribute>
				  	<attribute qualifier="height" type="double">
				  		<description>Height of the product, this attribute is dynamic to make it easier 
						to get the value from the classification system</description>
				    	<persistence type="dynamic"/>
				    	<modifiers write="false"/>
				  	</attribute>
				  	<attribute qualifier="width" type="double">
				  		<description>Width of the product, this attribute is dynamic to make it easier 
						to get the value from the classification system</description>
				    	<persistence type="dynamic"/>
				    	<modifiers write="false"/>
				  	</attribute>
				</attributes>
			</itemtype>
			<itemtype code="BabyartikelSizeVariantProduct" extends="BabyartikelProduct"
			          autocreate="true" generate="true"
			          jaloclass="de.kpfamily.core.jalo.BabyartikelSizeVariantProduct">
				<description>Babyartikel size variant type that contains additional attribute describing variant size.</description>
				<attributes>
					<attribute qualifier="size" type="localized:java.lang.String" metatype="VariantAttributeDescriptor">
						<description>Size of the product.</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>
			<itemtype code="BabyartikelSerialVoucherProduct" extends="BabyartikelProduct" 
				jaloclass="de.kpfamily.core.jalo.BabyartikelSerialVoucherProduct">
				<attributes>
					<attribute qualifier="serialVoucher" type="SerialVoucher" generate="true">
						<description>serial voucher from voucher extension</description>
						<persistence type="property"/>
						<modifiers optional="false"/>
					</attribute>
				</attributes>
			</itemtype>
			<itemtype code="BabyartikelSerialVoucherProductSellingHistory" 
				jaloclass="de.kpfamily.core.jalo.BabyartikelSerialVoucherProductSellingHistory">
				<deployment table="SVProductSellingHistory" typecode="10003"/>
				<attributes>
					<attribute qualifier="serialVoucherCode" type="java.lang.String">
						<description>serial voucher code that was generated for a given order entry</description>
						<persistence type="property"/>
						<modifiers optional="false"/>
					</attribute>
					<attribute qualifier="orderEntry" type="OrderEntry">
						<description>the order entry that the serial voucher was generated for</description>
						<persistence type="property"/>
						<modifiers optional="false"/>
					</attribute>
				</attributes>
			</itemtype>
			<itemtype code="ArticleState" generate="false" autocreate="false">
				<attributes>
					<attribute qualifier="internalCode" type="java.lang.String" generate="false">
						<description>The internal code used to identify the Article state, used by the company team to daily maintain the product data.</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>
			<itemtype code="ShippingMethod" generate="false" autocreate="false">
				<attributes>
					<attribute qualifier="internalCode" type="java.lang.String" generate="false">
						<description>The internal code used to identify the Shipping method, used by the company team to daily maintain the product data.</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>
			<itemtype code="PackageSize">
				<deployment table="PackageSize" typecode="10000"/>
				<attributes>
					<attribute qualifier="tagCode" type="java.lang.String">
						<description>Tag code used to identify the package</description>
						<modifiers unique="true"/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="maxLength" type="int">
						<description>Max length that this package supports in cm</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="maxWidth" type="int">
						<description>Max width that this package supports in cm</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="maxHeight" type="int">
						<description>Max height that this package supports in cm</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>
			<itemtype code="ProductSupplier">
				<description>Products supplier</description>
				<deployment table="ProductSupplier" typecode="10001"/>
				<attributes>
					<attribute type="java.lang.String" qualifier="code">
						<modifiers optional="false" unique="true" />
						<persistence type="property"/>
					</attribute>
					<attribute type="localized:java.lang.String" qualifier="name">
						<modifiers optional="false" />
						<persistence type="property"/>
					</attribute>
					<attribute type="int" qualifier="orderFrequency">
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute type="int" qualifier="warehouseDeliveryDays">
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute type="int" qualifier="dropShippingDeliveryDays">
						<modifiers/>
						<persistence type="property"/>
					</attribute>
				</attributes>
				<indexes>
					<index name="supplierCodeIdx">
						<key attribute="code" />
					</index>
				</indexes>
			</itemtype>
		</typegroup>

		<!-- TypeGroups are for structure of this file only -->

		<typegroup name="Apparel">
			<itemtype code="ApparelProduct" extends="Product"
			          autocreate="true" generate="true"
			          jaloclass="de.kpfamily.core.jalo.ApparelProduct">
				<description>Base apparel product extension that contains additional attributes.</description>
				<attributes>
					<attribute qualifier="genders" type="GenderList">
						<description>List of genders that the ApparelProduct is designed for</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>

			<itemtype code="ApparelStyleVariantProduct" extends="VariantProduct"
			          autocreate="true" generate="true"
			          jaloclass="de.kpfamily.core.jalo.ApparelStyleVariantProduct">
				<description>Apparel style variant type that contains additional attribute describing variant style.
				</description>
				<attributes>
					<attribute qualifier="style" type="localized:java.lang.String"
					           metatype="VariantAttributeDescriptor">
						<description>Color/Pattern of the product.</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>

					<attribute qualifier="swatchColors" type="SwatchColorSet">
						<description>A normalized color mapping to a standardized front-end navigable name.
						</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
				</attributes>

			</itemtype>

			<itemtype code="ApparelSizeVariantProduct" extends="ApparelStyleVariantProduct"
			          autocreate="true" generate="true"
			          jaloclass="de.kpfamily.core.jalo.ApparelSizeVariantProduct">
				<description>Apparel size variant type that contains additional attribute describing variant size.
				</description>
				<attributes>
					<attribute qualifier="size" type="localized:java.lang.String"
					           metatype="VariantAttributeDescriptor">
						<description>Size of the product.</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>
		</typegroup>
		
		<typegroup name="Electronics">
			<itemtype code="ElectronicsColorVariantProduct" extends="VariantProduct"
          autocreate="true" generate="true"
          jaloclass="de.kpfamily.core.jalo.ElectronicsColorVariantProduct">
				 <description>Electronics color variant type that contains additional attribute describing variant color.
				</description>
				<attributes>
					<attribute qualifier="color" type="localized:java.lang.String"
					           metatype="VariantAttributeDescriptor">
						<description>Color of the product.</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>
		</typegroup>

		<typegroup name="Processes">
			<itemtype code="OrderEntryStatusProcess" extends="BusinessProcess"
					autocreate="true" generate="true"
					jaloclass="de.kpfamily.core.jalo.process.OrderEntryStatusProcess">
					<description>It represents the business process that updates an order entry.</description>
					<attributes>
						<attribute qualifier="orderEntry" type="OrderEntry">
							<persistence type="property" />
							<description>Attribute contains the order entry object that will be used in the process.</description>
						</attribute>
						<attribute qualifier="oldStatus" type="OrderEntryStatus">
							<persistence type="property" />
							<description>Attribute contains the old status of the order entry that will be used in the process.</description>
						</attribute>
						<attribute qualifier="newStatus" type="OrderEntryStatus">
							<persistence type="property" />
							<description>Attribute contains the new status of the order entry that will be used in the process.</description>
						</attribute>
					</attributes>
				</itemtype>
		</typegroup>
	</itemtypes>
</items>
