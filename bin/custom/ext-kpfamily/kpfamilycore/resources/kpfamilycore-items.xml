<items xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="items.xsd">

	<collectiontypes>
		<collectiontype code="GenderList" elementtype="Gender" autocreate="true" generate="true" type="list"/>
		<collectiontype code="SwatchColorSet" elementtype="SwatchColorEnum" autocreate="true" generate="true" type="set"/>
	</collectiontypes>
	
	<enumtypes>
		<enumtype generate="true" code="SwatchColorEnum" autocreate="true" dynamic="true">
			<value code="BLACK"/>
			<value code="BLUE"/>
			<value code="BROWN"/>
			<value code="GREEN"/>
			<value code="GREY"/>
			<value code="ORANGE"/>
			<value code="PINK"/>
			<value code="PURPLE"/>
			<value code="RED"/>
			<value code="SILVER"/>
			<value code="WHITE"/>
			<value code="YELLOW"/>
		</enumtype>
		<enumtype generate="true" code="ArticleState" autocreate="true">
			<value code="CURRENT"/>
			<value code="DEPRECATED"/>
			<value code="OLD"/>
		</enumtype>
		<enumtype generate="true" code="ShippingMethod" autocreate="true">
			<value code="WAREHOUSE"/>
			<value code="DROPSHIPPING"/>
		</enumtype>
		<enumtype code="OrderStatus" autocreate="false" generate="true" dynamic="true">
			<value code="RETURNED"/>
			<value code="CHARGEBACK"/>
		</enumtype>
		<enumtype code="OrderEntryStatus" autocreate="false" generate="true" dynamic="false">
			<value code="CHARGEBACK"/>
			<value code="SHIPPED"/>
			<value code="RETURNED"/>
			<value code="AWAITING_PAYMENT"/>
			<value code="AWAITING_SHIPPING"/>
			<value code="PROCESSING"/>
			<value code="NOT_AVAILABLE"/>
			<value code="DIRECT_SHIPMENT"/>
			<value code="DIRECT_SHIPMENT_PACKED"/>
			<value code="EXCHANGE"/>
		</enumtype>
	</enumtypes>
	
	<relations>
		<relation code="Product2ProductSupplierRelation" autocreate="true" generate="false" localized="false">
			<deployment table="Product2SupplierRel" typecode="10002" />
			<sourceElement type="BabyartikelProduct" cardinality="many" navigable="false" />
			<targetElement type="ProductSupplier" qualifier="suppliers" collectiontype="list" cardinality="many"/>
		</relation>
		<relation code="Product2SupplierPriceHistoryRelation" localized="false">
			<sourceElement type="BabyartikelProduct" qualifier="product" cardinality="one"/>
			<targetElement type="PriceHistoryEntry" qualifier="supplierPriceHistory" cardinality="many"/>
		</relation>
		<relation code="Product2SellingPriceHistoryRelation" localized="false">
			<sourceElement type="BabyartikelProduct" qualifier="product" cardinality="one"/>
			<targetElement type="PriceHistoryEntry" qualifier="sellingPriceHistory" cardinality="many"/>
		</relation>
		<relation code="Product2ProductSalesBreakdownRecordRelation" localized="false">
			<sourceElement type="BabyartikelProduct" qualifier="product" cardinality="one"/>
			<targetElement type="ProductSalesBreakdownRecord" qualifier="salesBreakdown" cardinality="many"/>
		</relation>
		<relation code="ProductLogisticData2ProductPackageRelation" localized="false">
			<sourceElement type="ProductLogisticData" qualifier="logisticData" cardinality="one"/>
			<targetElement type="ProductPackage" qualifier="packages" cardinality="many"/>
		</relation>
	</relations>

	<itemtypes>
		<typegroup name="Hybris">
			 <itemtype code="AbstractOrderEntry" autocreate="false" generate="false">
				<attributes>
		            <attribute qualifier="status" type="OrderEntryStatus">
						<modifiers />						
						<persistence type="property" />
					</attribute>
				</attributes>
			</itemtype>
			<itemtype code="OrderEntryStatus" generate="false" autocreate="false">
				<attributes>
					<attribute qualifier="externalCode" type="java.lang.String" generate="false">
						<modifiers/>
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>
			<itemtype code="StockLevel" generate="false" autocreate="false">
				<attributes>
					<attribute qualifier="minimumStockLevel" type="int">
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="warehouseArrivalDate" type="java.util.Date">
						<description>Only for out of stock items: this date indicates when new items will arrive at the warehouse</description>
						<persistence type="property"/>
						<modifiers />
					</attribute>
				</attributes>
			</itemtype>
		</typegroup>
		
		<typegroup name="Babyartikel">
			<itemtype code="BabyartikelProduct" extends="Product">
				<description>Base babyartikel product extension that contains additional attributes.</description>
				<attributes>
					<attribute qualifier="shortDescription" type="localized:java.lang.String">
						<description> A short description of the product. </description>
						<persistence type="property">
							<columntype database="mysql">
								<value>text</value>
							</columntype>
						</persistence>
						<modifiers read="true" write="true" optional="true"/>
					</attribute>
					<attribute qualifier="articleState" type="ArticleState">
						<description>Defines the current state of the product within it's collection</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="articleStateInternalCode" type="java.lang.String">
						<description>Dynamic attribute to retrieve the article state internal code</description>
						<modifiers read="true" write="false" />
						<persistence type="dynamic" attributeHandler="articleStateInternalCodeHandler"/>
					</attribute>
					<attribute qualifier="supplierPrice" type="double">
						<description>The price that the product was bought from the supplier - old manufacturerPurchasePrice</description>
						<persistence type="property"/>
						<modifiers />
					</attribute>
					<attribute qualifier="weight" type="double">
						<description>Weight of the product, this attribute is dynamic to make it easier 
						to get the value from the classification system</description>
				    	<persistence type="dynamic" attributeHandler="babyartikelProductWeightHandler"/>
				    	<modifiers write="false"/>
				  	</attribute>
				  	<attribute qualifier="length" type="double">
				  		<description>Length of the product, this attribute is dynamic to make it easier 
						to get the value from the classification system</description>
				    	<persistence type="dynamic" attributeHandler="babyartikelProductLengthHandler"/>
				    	<modifiers write="false"/>
				  	</attribute>
				  	<attribute qualifier="height" type="double">
				  		<description>Height of the product, this attribute is dynamic to make it easier 
						to get the value from the classification system</description>
				    	<persistence type="dynamic" attributeHandler="babyartikelProductHeightHandler"/>
				    	<modifiers write="false"/>
				  	</attribute>
				  	<attribute qualifier="width" type="double">
				  		<description>Width of the product, this attribute is dynamic to make it easier 
						to get the value from the classification system</description>
				    	<persistence type="dynamic" attributeHandler="babyartikelProductWidthHandler"/>
				    	<modifiers write="false"/>
				  	</attribute>
				  	<attribute qualifier="seoPageTitle" type="localized:java.lang.String">
						<modifiers />
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="reviewsAverageRating" type="double">
						<modifiers />
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="reviewsTotalCount" type="int">
						<modifiers />
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="previousPrice" type="double">
						<modifiers />
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="suggestedRetailPrice" type="double">
						<modifiers />
						<persistence type="property"/>
					</attribute>
					<attribute type="int" qualifier="dailySalesFrequency">
						<description>old daily sales.</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="tradingGroup" type="TradingGroup">
						<modifiers />
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="family" type="java.lang.String">
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="designFamily" type="java.lang.String">
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="gaPageViews" type="int">
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute type="ProductLogisticData" qualifier="logisticData">
						<modifiers/>
						<persistence type="property"/>
					</attribute>
				  	<attribute qualifier="defaultStockLevel" type="StockLevel">
				    	<persistence type="dynamic" attributeHandler="babyartikelProductDefaultStockLevelHandler"/>
				    	<modifiers write="false"/>
				  	</attribute>
				</attributes>
			</itemtype>
			<itemtype code="BabyartikelSizeVariantProduct" extends="BabyartikelProduct"
			          autocreate="true" generate="true"
			          jaloclass="de.kpfamily.core.jalo.BabyartikelSizeVariantProduct">
				<description>Babyartikel size variant type that contains additional attribute describing variant size.</description>
				<attributes>
					<attribute qualifier="size" type="localized:java.lang.String" metatype="VariantAttributeDescriptor">
						<description>Size of the product.</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>
			<itemtype code="BabyartikelSerialVoucherProduct" extends="BabyartikelProduct" 
				jaloclass="de.kpfamily.core.jalo.BabyartikelSerialVoucherProduct">
				<attributes>
					<attribute qualifier="serialVoucher" type="SerialVoucher" generate="true">
						<description>serial voucher from voucher extension</description>
						<persistence type="property"/>
						<modifiers optional="false"/>
					</attribute>
				</attributes>
			</itemtype>
			<itemtype code="BabyartikelSerialVoucherProductSellingHistory" 
				jaloclass="de.kpfamily.core.jalo.BabyartikelSerialVoucherProductSellingHistory">
				<deployment table="SVProductSellingHistory" typecode="10003"/>
				<attributes>
					<attribute qualifier="serialVoucherCode" type="java.lang.String">
						<description>serial voucher code that was generated for a given order entry</description>
						<persistence type="property"/>
						<modifiers optional="false"/>
					</attribute>
					<attribute qualifier="orderEntry" type="OrderEntry">
						<description>the order entry that the serial voucher was generated for</description>
						<persistence type="property"/>
						<modifiers optional="false"/>
					</attribute>
				</attributes>
			</itemtype>
			<itemtype code="ArticleState" generate="false" autocreate="false">
				<attributes>
					<attribute qualifier="internalCode" type="java.lang.String" generate="false">
						<description>The internal code used to identify the Article state, used by the company team to daily maintain the product data.</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>
			<itemtype code="ShippingMethod" generate="false" autocreate="false">
				<attributes>
					<attribute qualifier="internalCode" type="java.lang.String" generate="false">
						<description>The internal code used to identify the Shipping method, used by the company team to daily maintain the product data.</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>
			<itemtype code="ProductPackage">
				<deployment table="ProductPackage" typecode="10008"/>
				<attributes>
					<attribute qualifier="tagCode" type="java.lang.String">
						<description>Tag code used to identify the package</description>
						<modifiers read="true" write="false" />
						<persistence type="dynamic" attributeHandler="productPackageTagCodeHandler"/>
					</attribute>
					<attribute qualifier="bulky" type="boolean">
						<description>Defines if the product is bulky</description>
						<modifiers read="true" write="false" />
						<persistence type="dynamic" attributeHandler="productPackageBulkyHandler"/>
					</attribute>
					<attribute qualifier="length" type="int">
						<description>Length that this package supports in cm</description>
						<modifiers optional="false"/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="width" type="int">
						<description>Width that this package supports in cm</description>
						<modifiers optional="false"/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="height" type="int">
						<description>Height that this package supports in cm</description>
						<modifiers optional="false"/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="weight" type="double">
						<description>Weight that this package supports</description>
						<modifiers optional="false"/>
						<persistence type="property"/>
					</attribute>
				</attributes>
			</itemtype>
			<itemtype code="ProductSupplier">
				<description>Products supplier</description>
				<deployment table="ProductSupplier" typecode="10001"/>
				<attributes>
					<attribute type="java.lang.String" qualifier="code">
						<modifiers optional="false" unique="true" />
						<persistence type="property"/>
					</attribute>
					<attribute type="localized:java.lang.String" qualifier="name">
						<modifiers optional="false" />
						<persistence type="property"/>
					</attribute>
					<attribute type="int" qualifier="warehouseDeliveryDays">
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute type="int" qualifier="dropShippingDeliveryDays">
						<description>old dvDeliveryDays.</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="orderFrequency" type="int">
						<modifiers/>
						<persistence type="property"/>
					</attribute>
				</attributes>
				<indexes>
					<index name="supplierCodeIdx">
						<key attribute="code" />
					</index>
				</indexes>
			</itemtype>
			<itemtype code="TradingGroup">
				<deployment table="TradingGroup" typecode="10004"/>
				<attributes>
					<attribute qualifier="code" type="java.lang.String">
						<modifiers unique="true" optional="false" />
						<persistence type="property" />
					</attribute>
					<attribute qualifier="name" type="localized:java.lang.String">
						<modifiers />
						<persistence type="property" />
					</attribute>
				</attributes>
				<indexes>
					<index name="tradingGroupCodeIDX" unique="true">
						<key attribute="code"/>
					</index>
				</indexes>
			</itemtype>
			<itemtype code="PriceHistoryEntry">
				<deployment table="PriceHistoryEntry" typecode="10005"/>
				<attributes>
					<attribute qualifier="price" type="double">
						<persistence type="property"/>
						<modifiers optional="false" initial="true" write="false" />
					</attribute>
					<attribute qualifier="date" type="java.util.Date" >
						<persistence type="property"/>
						<modifiers optional="false" initial="true" write="false" />
					</attribute>
				</attributes>
			</itemtype>
			<itemtype code="ProductSalesBreakdownRecord">
				<deployment table="ProductSalesBreakdown" typecode="10006"/>
				<attributes>
					<attribute qualifier="timePeriodInDays" type="int">
						<persistence type="property"/>
						<modifiers optional="false" />
					</attribute>
					<attribute qualifier="quantity" type="int">
						<persistence type="property"/>
						<modifiers optional="false"/>
					</attribute>
					<attribute qualifier="source" type="java.lang.String" >
						<persistence type="property"/>
						<modifiers optional="false" />
					</attribute>
				</attributes>
			</itemtype>
			<itemtype code="ProductLogisticData">
				<deployment table="ProductLogisticData" typecode="10007"/>
				<attributes>
					 <attribute qualifier="productCode" type="java.lang.String">
					      <modifiers optional="false" unique="true" initial="true" write="false"/>
					      <persistence type="property"/>
					 </attribute>
					<attribute type="double" qualifier="customsTariffNumber">
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute type="Country" qualifier="countryOfOrigin">
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute type="boolean" qualifier="dispatchViaForwardingAgent">
						<description>defines if the product is so big that we cannot dispatch it via package, but via 'truck', old name: forwardingShipping</description>
						<modifiers optional="false"/>
						<persistence type="property"/>
						<defaultvalue>false</defaultvalue>
					</attribute>
					<attribute type="boolean" qualifier="dispatchViaSupplier">
						<description>defines if the product will be delivered directly from the Supplier to the final Customer, old name: dfMethod</description>
						<modifiers optional="false"/>
						<persistence type="property"/>
						<defaultvalue>false</defaultvalue>
					</attribute>
					<attribute qualifier="shippingMethod" type="ShippingMethod">
						<description>Defines the way that the product is shipped</description>
						<modifiers/>
						<persistence type="property"/>
					</attribute>
					<attribute qualifier="shippingMethodInternalCode" type="java.lang.String">
						<description>Dynamic attribute to retrieve the shipping method internal code</description>
						<modifiers read="true" write="false" />
						<persistence type="dynamic" attributeHandler="shippingMethodInternalCodeHandler"/>
					</attribute>
				</attributes>
				<indexes>
					<index name="productLogisticDataProductCodeIDX" unique="true">
						<key attribute="productCode"/>
					</index>
				</indexes>
			</itemtype>
		</typegroup>

